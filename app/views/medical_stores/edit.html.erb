<section class="content-header">
  <h1>
    edit Medical Store
  </h1>
</section><br/>
<div class="box box-default">
  <div class="box-header with-border">
    <h3 class="box-title">Medical Store Info For Hospital : <%= @current_hospital.name %></h3>
  </div>
  <%= form_for @medical_store, :url=> hospital_medical_store_path, html: { method: :put}  do |f| %>
    <div class="box-body">
      <%= render :partial => 'form', locals:{f: f} %>
    </div>
    <div class="box-footer">
      <%= f.submit 'Welcome Medical Partner', class: 'btn btn-success' %>
    </div>
  <% end %>
</div>
